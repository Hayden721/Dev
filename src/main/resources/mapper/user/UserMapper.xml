<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dev.shop.user.dao.UserDao">

    <select id="selectEmailPw" resultType="int" parameterType="User">
        select count(*)
        from user_tb
        where 1=1
        and user_email = #{userEmail}
        and user_pw = #{userPw}
    </select>

</mapper>